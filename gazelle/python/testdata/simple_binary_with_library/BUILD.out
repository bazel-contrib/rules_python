load("@rules_python//python:defs.bzl", "py_binary", "py_library")

py_library(
    name = "simple_binary_with_library",
    srcs = [
        "__init__.py",
        "bar.py",
        "foo.py",
    ],
)

# This target should be kept unmodified by Gazelle.
py_library(
    name = "custom",
    srcs = [
        "bar.py",
    ],
)

py_binary(
    name = "simple_binary_with_library_bin",
    srcs = ["__main__.py"],
    main = "__main__.py",
    deps = [":simple_binary_with_library"],
)
